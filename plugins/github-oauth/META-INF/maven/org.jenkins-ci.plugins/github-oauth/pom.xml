      <artifactId>mailer</artifactId>
      <version>414.vcc4c33714601</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>org.jenkins-ci.plugins</groupId>
      <artifactId>github-api</artifactId>
      <version>1.303-400.v35c2d8258028</version>
